%!PS
%%Title: SMF Borehole Specimen
%%Creator: Michael D. Mazzone
%%CreationDate: Apr. 24, 1995
%%EndComments

%  John Dey Photography Gallery Stamp
%  two stamps per sheet landscape
%
%  8.5 in = 612 pt
%  11 in = 792 pt
%
%  Defines
/in { 72 mul } bind def

/Helvetica findfont 50 scalefont setfont

% usage: [x] [y] <text lable> <barcode> makelabel
/width { 3.75 72 mul } bind def
%
%  Height is Logo Height only
/height { 1.75 72 mul } bind def
/halfH { height 2 div } bind def

/box {
  % re-factor to make x y upper left
  /y exch def
  /x exch def
  % outer Box
  x y moveto
  1 setlinewidth
  width  0   rlineto 
  0 4 in neg rlineto
  width neg 0  rlineto
  closepath stroke
  % Lower black box "Photography"
  x y height sub moveto
  0 height 2 div rlineto
  width 0 rlineto 
  0 height 2 div neg rlineto fill
  %
  % draw a few lines
  .5 setlinewidth
  /lineheight 48 def
  x y height sub lineheight sub moveto
  width 0 rlineto closepath stroke
  x y height sub lineheight 2 mul sub moveto
  width 0 rlineto closepath stroke
  x y height sub lineheight 3 mul sub moveto
  width 0 rlineto closepath stroke

x y height sub moveto 8 16 rmoveto  %Lower left
1 setgray
{ -3.5 0 rmoveto } (Photography) kshow

0 setgray
x y height 2 div sub moveto 6 16 rmoveto  %Lower left
{ 6.5 0 rmoveto } (John Dey) kshow

} def


% Max height 792 - 36 - 126 = 630 
% Max width (612 / 2) +2 = 308
 16 720 box
308 720 box

% Max height 720 / 2  = 360 - 126 = 234  (792 / 3 = 264 )
 16 360 box
308 360 box

showpage
