%!PS-Adobe-2.0
%%Title: SMF Borehole Specimen
%%Creator: Michael D. Mazzone
%%CreationDate: Apr. 24, 1995
%%EndComments

%  8.5 in = 612 pt
%  11 in = 792 pt
%
%  Defines

/in { 72 mul } bind def

% Print logo box and text
% [x] [y] [width] [height] are inputs
% x y are lower left of logo box
% width height are box boundrys
/logo {
  % Draw logo box
  /y exch def
  /x exch def
  /dx x .25 in add def
  /dy y 2 in height sub 2 div add def
  /half height 2 div def
  /baseline half .24 mul def
  /skip width .05 mul def
  /halfskip skip 2 div def
  1 setlinewidth
  0 setgray
  dx dy moveto
  width  0   rlineto 
  0 height   rlineto
  width neg  0  rlineto
  closepath stroke
  dx dy moveto
  0 half  rlineto
  width 0 rlineto 
  0 half neg rlineto fill
  % Draw logo Text
  /Helvetica findfont half .85 mul scalefont setfont
    % Top
      0 setgray
      dx dy moveto
      /kern width (JohnJoanie) stringwidth pop sub skip sub 8 div def 
      halfskip  half baseline add rmoveto
    { kern 0 rmoveto } (JohnJoanie) kshow
    % Bottom
      dx dy moveto
      1 setgray
      /kern width (Photography) stringwidth pop sub skip sub 11 div def 
      halfskip kern add baseline rmoveto
      { kern 0 rmoveto } (Photography) kshow
    0 setgray
    %% Phone number
    %save
    %/Courier findfont 12 scalefont setfont
    %%x 3.5 in add y 1 in add tick 
    %x 3.15 in add y 1 in add moveto
    %90 rotate
    %(360.649.2731) dup  stringwidth pop 2 div neg 0 rmoveto show
    %restore
} def


/width 4.5 in def
/height 2 in def
/x1 1.75 in def 
/y1 2 in def
x1 y1 logo

showpage
%%EOF
